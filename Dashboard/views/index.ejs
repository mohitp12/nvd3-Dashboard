    <script>
    
    
    var app = angular.module('plunker', ['nvd3'])

    .controller('MainCtrl', function($scope,$http,$interval) {
        $scope.options = {
            chart: {
                type: 'lineChart',
                height: 370,
                margin : {
                    top: 20,
                    right: 20,
                    bottom: 40,
                    left: 55
                },
                x: function(d){ return d.x; },
                y: function(d){ return d.y; },
                useInteractiveGuideline: true,
                duration: 500,    
                yAxis: {
                    tickFormat: function(d){
                       return d3.format('.01f')(d);
                    }
                }
            }
        };
        
        $scope.options1 = angular.copy($scope.options);
        $scope.options1.chart.duration = 0;
        $scope.options1.chart.yDomain = [0,100];
        
        $scope.data = [{ values: [], key: 'Sensor Data' }];
            
        $scope.run = true;
        
        $interval(callAtInterval,1000);
        
        function callAtInterval(){
        $http({
    		method:"GET",
    		url:'/getData'
    	}).then(function(res){
    		console.log(res);
    		if(res.data.status=="200"){
    			console.log(res.data.x+"::"+res.data.y);
    			 $scope.data[0].values.push({ x: res.data.x,y: res.data.y});
    			if ($scope.data[0].values.length > 20) $scope.data[0].values.shift();
              $scope.$apply();
    		}
    	})
        }
    });

    
    </script>
